# Copy to .env and fill values
NOTION_API_KEY=ntn_xxxxxxxxxxxxxxxxxxxxxxxxx
NOTION_DATABASE_ID=NOTION_DATABASE_ID_PLACEHOLDER
# Set to 'true' to use CSV fallback mode
USE_CSV=false

# Google Maps API Key (optional but recommended for better Ukrainian geocoding)
# Get your API key at: https://console.cloud.google.com/apis/credentials
# Enable "Geocoding API" in Google Cloud Console
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
